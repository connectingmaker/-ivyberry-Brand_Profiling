<input type="hidden" id="campaign_code" name="campaign_code" value="<%=campaign_code%>">
<input type="hidden" id="seq" name="seq" value="<%=seq%>">
<input type="hidden" id="uid" name="uid" value="<%=uid%>">
<input type="hidden" id="quest_num" value="<%=quest_num%>">
<input type="hidden" id="q_code" value="<%=surveyQ.Q_CODE%>">
<input type="hidden" id="page" value="<%=page%>">
<input type="hidden" id="brandCnt" value="<%=brandList.length%>">
<input type="hidden" id="debug" value="<%=debug%>">
    <input type="hidden" id="brandSelect" value="1">
    <% var brandNumCheck = 1; %>
    <% for(var brandNum = 0; brandNum<brandList.length; brandNum++) { %>
    <div class="row" id="brand<%=brandNumCheck%>" class="brandView" style="padding:10px 40px; <% if(brandNum != 0) { %>display:none;<%}%>">
        <div clss="col-xs-12">
            <div class="row titleBox2">

                <div class="col-xs-12 text-center titleCountBg">
                    <h2> <span id="selectCnt" class="Textcolor"><%=brandNumCheck%></span> / <%=brandList.length%></h2>

                    <small><span id="textselectCnt_<%=brandNum%>" class="Textcolor">0개가 선택되었습니다</span></small>
                </div>

                <div class="col-xs-12 text-left titleSelectBg" style="padding-left:10px;font-size:1.2em;">
                    <%=brandList[brandNum].BRAND_NAME_KO%> - <%=surveyQ.Q_TITLE_KO%>
                </div>
            </div>

        </div>

        <div clss="col-xs-12" style="padding:30px 0px;">


            <div class="row">
                <% for(var i = 0; i<surveyQA.length; i++) { %>

                <div class="col-xs-6">
                    <input type='checkbox' id="qaData_<%=brandNum%><%=i%>" name="qa[<%=brandNum%>]" brand_code="<%=brandList[brandNum].DETAIL_BRAND_CODE%>" class="qaData qaData_<%=brandNum%>" value="<%=surveyQA[i].QA_CODE%>" style="display:none;">
                    <button class="qaList btn btn-white push-5-r push-10 btn-block btn-md" brandNum="<%=brandNum%>" qaCode="<%=i%>" type="button">
                    <span class="pull-left"><img src="/img/survey/checkBox_icon_off.png">&nbsp;&nbsp;&nbsp;<%=surveyQA[i].QA_TITLE_KO%></span>
                    </button>
                </div>

                <% } %>

            </div>

        </div>
    </div>

    <% brandNumCheck++; %>
    <% } %>
</div>
<script language="javascript" src="/view_js/survey/5.js"></script>