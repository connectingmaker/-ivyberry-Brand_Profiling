<div class="row" style="padding:0px;">
    <div class="block" style="margin-bottom:-20px;">
        <div class="block-content" style="padding:0px;">
            <div class="progress progress-mini" style="background-color:#fff;">
                <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="<%=total_per%>" aria-valuemin="0" aria-valuemax="100" style="width: <%=total_per%>%;"></div>
            </div>
        </div>
    </div>
</div>


<input type="hidden" id="campaign_code" name="campaign_code" value="<%=campaign_code%>">
<input type="hidden" id="seq" name="seq" value="<%=seq%>">
<input type="hidden" id="uid" name="uid" value="<%=uid%>">
<input type="hidden" id="quest_num" value="<%=quest_num%>">
<input type="hidden" id="q_code" value="<%=surveyQ.Q_CODE%>">
<input type="hidden" id="page" value="<%=page%>">
<input type="hidden" id="brandCnt" value="<%=brandList.length%>">

<input type="hidden" id="q_img_min" value="<%=surveyQ.Q_IMG_MIN%>">
<input type="hidden" id="q_img_max" value="<%=surveyQ.Q_IMG_MAX%>">
<input type="hidden" id="q_text_min" value="<%=surveyQ.Q_TEXT_MIN%>">
<input type="hidden" id="q_text_max" value="<%=surveyQ.Q_TEXT_MAX%>">
<input type="hidden" id="debug" value="<%=debug%>">
<input type="hidden" id="lang" value="<%=lang%>">

<div class="row" style="padding:10px 40px;">
    <% for(var brandNum = 0; brandNum<brandList.length; brandNum++) { %>

    <% if(brandNum == 0) { %>
        <div id="brandView_<%=brandNum%>">
    <% } else { %>
        <div id="brandView_<%=brandNum%>" style="display:none;">
    <% } %>
            <div class="brandImg_<%=brandNum%> brandImg">
                <div clss="col-xs-12">
                    <div class="row titleBox2">

                        <div class="col-xs-12 text-center titleCountBg">
                            <h2> <span id="selectCnt" id="selectCnt" class="Textcolor">0</span> / 1</h2>

                        </div>

                        <div class="col-xs-12 text-center titleSelectBg" style="font-size:1.2em;">
                            <b class="Textcolor">
                                <% if(lang == "en") { %>
                                <%=brandList[brandNum].BRAND_NAME_EN%>
                                <% } else if(lang == "zh") { %>
                                <%=brandList[brandNum].BRAND_NAME_CN%>
                                <% } else { %>
                                <%=brandList[brandNum].BRAND_NAME_KO%>
                                <% } %>
                                - </b>
                                <% if(lang == "en") { %>
                                <%=surveyQ.Q_TITLE_EN%>
                                <% } else if(lang == "zh") { %>
                                <%=surveyQ.Q_TITLE_CN%>
                                <% } else { %>
                                <%=surveyQ.Q_TITLE_KO%>
                                <% } %>
                                <% if(lang == "en") { %>
                                     <span class="TextColor">(1 select)</span>
                                <% } else if(lang == "zh") { %>
                                     <span class="TextColor">(1)</span>

                                <% } else { %>
                                    <span class="TextColor">(1개)</span>
                                <% } %>

                        </div>
                    </div>

                </div>

                <div clss="col-xs-12" style="padding:10px 0px;">
                    <div class="push-20-t text-center">
                    <table border="0" width="100%" cellspacing="0" cellpadding="0">
                        <col width="49%">
                        <col width="2%">
                        <col width="49%">
                    <% for(var i = 0; i<surveyQA.length; i++) { %>
                        <%
 console.log(surveyQA);
                        %>
                        <% if(i%2 == 0) { %>
                        <tr>
                            <td class="imgBox_off imgBox" brand_code="<%=brandList[brandNum].DETAIL_BRAND_CODE%>" qa_code="<%=surveyQA[i].QA_CODE%>" align="center">
                                <span class="imgBox_check"><img src="/img/survey/checkBox_icon_off.png"></span>
                                <img src="<%=surveyQA[i].IMG%>" width="100%" align="center" style="padding-top:10px;">
                            </td>
                        <% } %>


                        <% if(i%2 == 1) { %>
                            <td></td>
                            <td class="imgBox_off imgBox" brand_code="<%=brandList[brandNum].DETAIL_BRAND_CODE%>" qa_code="<%=surveyQA[i].QA_CODE%>" align="center">
                                <span class="imgBox_check"><img src="/img/survey/checkBox_icon_off.png"></span>
                                <img src="<%=surveyQA[i].IMG%>" width="100%" align="center" style="padding-top:10px;"></td>
                        </tr>
                        <% } %>
                    <% } %>
                    </table>
                    </div>


                </div>
            </div>

            <div class="brandText_<%=brandNum%> brandText" style="display:none;">
                <div clss="col-xs-12">
                    <div class="row titleBox2">

                        <div class="col-xs-12 text-center titleCountBg">
                            <h2> <span id="selectCnt" class="Textcolor">0</span> / <%=surveyQ.Q_TEXT_MAX%></h2>

                        </div>

                        <div class="col-xs-12 text-center titleSelectBg">

                            <small>
                                <% if(lang == "en") { %>
                                Please write 1 to 5 keywords that are reminiscent of the image below.
                                <% } else if(lang == "zh") { %>
                                请填写通过以下图像联想到的1到5个相关关键词

                                <% } else { %>
                                아래 이미지에서 연상되는 키워드를 1개~5개 적어 주세요.
                                <% } %>
                            </small>
                        </div>
                    </div>

                </div>

                <div clss="col-xs-12" style="padding:10px 0px;">
                    <div class="push-20-t text-center">
                        <table border="0" width="100%" cellspacing="0" cellpadding="0">
                            <col width="49%">
                            <col width="2%">
                            <col width="49%">
                            <tr>
                                <td class="imgBox_sub" align="center"></td>
                                <td></td>
                                <td valign="top">
                                    <% for(var textNum = 0; textNum<parseInt(surveyQ.Q_TEXT_MAX); textNum++) { %>
                                    <div class="col-xs-12 push-10-t">
                                        <input type="text" class="form-control imgText" placeholder="input text">
                                    </div>
                                    <% } %>

                                </td>
                            </tr>
                        </table>
                    </div>


                </div>
            </div>


    </div>
    <% } %>
</div>
<script language="javascript" src="/view_js/survey/3.js"></script>