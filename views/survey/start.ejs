<% if(DEBUG == "true") { %>
<script>
    alert("ERR_CODE:<%=ERR_CODE%>\n<%=ERR_MSG%>");

</script>

<% } else { %>
<script>
    setTimeout(function () {
        var json = {
            ERR_CODE : "<%=ERR_CODE%>"
        }

        postMessage(JSON.stringify(json), '*');
    }, 0);

</script>
<% } %>

<%=ERR_CODE%>