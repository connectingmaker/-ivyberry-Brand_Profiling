<div class="row">
    <div class="col-xs-12 text-center" style="padding:10px 40px; font-size:14px; background-color:#f1f1f1;">
        <% if(lang == "ko") { %>
            <%=contents.SUBJECT%>
        <% } %>
        <% if(lang == "en") { %>
        <%=contents.SUBJECT_EN%>
        <% } %>
        <% if(lang == "zh") { %>
        <%=contents.SUBJECT_CN%>
        <% } %>
    </div>
    <div class="col-xs-12" id="wrapper">
        <% if(lang == "ko") { %>
            <%-contents.CONTENTS%>
        <% } %>

        <% if(lang == "en") { %>
        <%-contents.CONTENTS_EN%>
        <% } %>

        <% if(lang == "zh") { %>
        <%-contents.CONTENTS_CN%>
        <% } %>
    </div>
</div>

<script>
    $(function() {

        $("img").each(function() {
            $(this).css({"width":"100%"});

        });
    })
</script>

<script type="text/javascript">
    var myScroll = new iScroll('wrapper');
</script>